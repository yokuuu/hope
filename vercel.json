{
    
    "version": 2,
  "builds": [
    {
      "src": "**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.njk",
      "use": "@vercel/static"
    }
  ],
     "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ],
}
